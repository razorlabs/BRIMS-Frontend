{
  "name": "seatbelt",
  "version": "0.0.0",
  "description": "LIMS Frontend Development",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "npm-run-all --parallel security test devserver open:index start-mockapi open:mockapi",
    "devserver": "webpack-dev-server --inline --hot --config ./webpack.config.dev.js --mode development",
    "test": "echo \"Test framework to be specified\"",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint security",
    "security": "nsp check",
    "extra-security": "retire --severity medium",
    "start-mockapi": "json-graphql-server src/api/db.js",
    "open:index": "open http://localhost:8080",
    "open:mockapi": "open http://localhost:3000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/razorlabs/Seatbelt.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/razorlabs/Seatbelt/issues"
  },
  "homepage": "https://github.com/razorlabs/Seatbelt#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "colors": "^1.2.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "json-graphql-server": "^2.1.1",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.2.1",
    "open": "0.0.5",
    "react-hot-loader": "^4.0.1",
    "retire": "^1.6.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.79.1"
  }
}
